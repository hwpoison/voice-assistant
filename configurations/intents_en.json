{
    "commands": {
        "open the console": {
            "run_process": "cmd.exe",
            "alternative": [
                "open terminal"
            ]
        },
        "suspend computer": {
            "press": "{SLEEP}"
        },
        "escape": {
            "press": "{ESC}",
            "alternative": [
                "exit"
            ]
        },
        "write {n}": {
            "write": "{}",
            "wait": true
        },
        "open new tab": {
            "context": "BROWSER",
            "press": "^t"
        },
        "close tab": {
            "press": "^w",
            "context": "BROWSER"
        },
        "reopen tab": {
            "press": "^T",
            "context": "BROWSER"
        },
        "reload page": {
            "press": "{BROWSER_REFRESH}",
            "context": "BROWSER"
        },
        "switch tab {n}": {
            "ctrl_num": "{}",
            "context": "BROWSER"
        },
        "ctrl {n}": {
            "ctrl_num": "{}"
        },
        "next tab": {
            "press": "^{TAB}",
            "context": "BROWSER"
        },
        "previous tab": {
            "press": "^+{TAB}",
            "context": "BROWSER"
        },
        "full screen": {
            "context": "BROWSER",
            "press": "{f}",
            "alternative": [
                "exit full screen"
            ]
        },
        "select all": {
            "press": "^a"
        },
        "erase": {
            "press": "{BACKSPACE}"
        },
        "undo": {
            "press": "^z"
        },
