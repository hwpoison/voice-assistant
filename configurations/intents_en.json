            "press": "{ENTER}",
            "alternative": [
                "accept"
            ]
        },
        "next desktop": {
            "press": "^#{RIGHT}"
        },
        "previous desktop": {
            "press": "^#{LEFT}"
        },
        "minimize windows": {
            "press": "#m"
        },
        "maximize windows": {
            "press": "!{TAB}",
            "alternative": [
                "maximize the windows"
            ]
        },
        "take a screenshot": {
            "press": "#{PRINTSCREEN}"
        },
        "next page": {
            "press": "{PGDN}"
        },
        "up page": {
            "press": "{PGUP}"
        },
        "search": {
            "press": "^f"
        },
        "down {n} twices": {
            "press": "{DOWN}"
        },
        "down": {
            "press": "{DOWN}"
        },
        "up": {
            "press": "{UP}",
            "wait": true,
            "voice": "all right"
        },
        "turn up volumen {n}": {
            "press": "{VOLUME_UP}"
        },
        "turn down volumen {n}": {
            "press": "{VOLUME_DOWN}"
        },
        "mute sound": {
            "press": "{VOLUME_MUTE}",
            "alternative": [
                "silenciar sonidos"
            ]
        },
