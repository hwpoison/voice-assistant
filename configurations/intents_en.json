            "press": "{MEDIA_NEXT}"
        },
        "previous song": {
            "press": "{MEDIA_PREV}"
        },
        "stop music": {
            "press": "{MEDIA_PLAY_PAUSE}",
            "alternative": [
                "pause",
                "pause music",
                "silent"
            ]
        },
        "play": {
            "press": "{MEDIA_PLAY_PAUSE}",
            "alternative": [
                "reanude music",
                "play again"
            ]
        },
        "play music": {
            "press": "{MEDIA_PLAY_PAUSE}"
        },
        "open the mail": {
            "run_process": "outlook.exe"
        },
        "go back": {
            "press": "{BROWSER_BACK}"
        },
        "refresh": {
            "press": "{F5}"
        },
        "close window": {
            "press": "!{F4}"
        },
        "open whatsapp": {
            "run_process": "https://web.whatsapp.com/",
            "voice": "opening whatsapp"
        },
        "open youtube": {
            "run_process": "https://youtube.com",
            "voice": "opening youtube"
        },
        "open the translator": {
            "run_process": "https://deepl.com"
        },
        "open the note pad": {
            "run_process": "notepad",
            "alternative": [
                "open notepad"
            ]
        },
        "open spotify": {
            "run_process": "https://open.spotify.com/",
            "alternative": [
                "i wanna hear music"
            ]
        }
